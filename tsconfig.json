{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",

    "paths": {
      "@/*": ["*"],

      "@src/*": ["src/*"],

      "@components/*": ["src/components/*"],

      "@misc/*": ["src/misc/*"],

      "@utility": ["src/utility/index.ts"],

      "@types": ["src/types/index.ts"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/nativewind-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
